    <skos:narrower rdf:resource="https://gams.uni-graz.at/o:fercan.arch#C.110400"/>
    <skos:narrower rdf:resource="https://gams.uni-graz.at/o:fercan.arch#C.110500"/>
    <skos:narrower rdf:resource="https://gams.uni-graz.at/o:fercan.arch#C.110600"/>
    <skos:narrower rdf:resource="https://gams.uni-graz.at/o:fercan.arch#C.110700"/>
    <skos:narrower rdf:resource="https://gams.uni-graz.at/o:fercan.arch#C.110800"/>
    <skos:narrower rdf:resource="https://gams.uni-graz.at/o:fercan.arch#C.110900"/>

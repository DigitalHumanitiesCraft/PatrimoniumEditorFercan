  <skos:Concept rdf:about="https://gams.uni-graz.at/o:fercan.arch#C.110000">
    <skos:prefLabel xml:lang="de"> Inschriftenträger</skos:prefLabel>
    <skos:inScheme rdf:resource="https://gams.uni-graz.at/o:fercan.arch"/>
    <skos:broader rdf:resource="https://gams.uni-graz.at/o:fercan.arch#C.100000"/>
    <skos:narrower rdf:resource="https://gams.uni-graz.at/o:fercan.arch#C.110100"/>
